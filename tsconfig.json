{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "Components/*": [
        "Components/*"
      ],
      "Pages/*": [
        "Pages/*"
      ],
      "Utils/*": [
        "Utils/*"
      ],
      "Hooks/*": [
        "Hooks/*"
      ],
      "Context/*": [
        "Context/*"
      ],
      "Store/*": [
        "Store/*"
      ],
      "Assets/*": [
        "Assets/*"
      ],
      "Services/*": [
        "Services/*"
      ],
      "Interfaces/*": [
        "Interfaces/*"
      ],
      "Navigation/*": [
        "Navigation/*"
      ]
    }
  },
}
